									"                                                \"name\": {\"type\": \"string\",\"pattern\": city}   //和入参关联\r",
									"                                                }                               \r",
									"                        },                         \r",
									"                       \"minItems\": 1,\r",
									"                    //    \"maxItems\": 2\r",
									"                       },\r",
