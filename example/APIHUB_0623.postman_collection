									"                                                \"citycode\": {\"type\": \"string\", \"maxLength\": 4},\r",
									"                                                \"districts\": {\"type\": \"array\"},\r",
									"                                                \"level\": {\"type\": \"string\", \"enum\": [\"district\", \"city\", \"province\"]},    //最好罗列下enmu的值city什么的\r",
									"                                                \"name\": {\"type\": \"string\", \"pattern\": city}   //这个地方怎么和入参关联也要学习一下\r",
									"                                                }\r",
									"                                },\r",
									"                        ],\r",
									"                        \"additionalItems\": {\r",
									"                                \"type\": \"object\",\r",
